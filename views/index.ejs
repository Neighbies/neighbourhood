<% if (!loggedIn) { %>
  <a href="/signup">Sign up</a>
  <a href="/login">Log in</a>
  <% } else { %>
    <a href="/logout">Log out</a>
    <h2>Categories</h2>

    <% categories.forEach(category => { %>
      <a href="/things/<%=category.id%>">
        <%= category.title %>
      </a>
      <% }); %>
        <% } %>